{
  "name": "lozemc/laravel-actions",
  "type": "library",
  "version": "11.0",
  "description": "Simple command to create actions in Laravel",
  "license": "MIT",
  "keywords": [
    "laravel",
    "action",
    "actions",
    "command",
    "commands"
  ],
  "homepage": "https://github.com/lozemc/",
  "authors": [
    {
      "name": "Lozemc",
      "email": "lozemc@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Lozemc\\Actions\\": "src/"
    }
  },
  "require": {
    "php": "^8.2|^8.3",
    "laravel/framework": "^11.0"
  },
  "config": {
    "platform-check": false,
    "optimize-autoloader": true
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10"
  }
}
